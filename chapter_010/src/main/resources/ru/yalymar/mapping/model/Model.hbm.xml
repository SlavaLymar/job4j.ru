<hibernate-mapping>
    <class name="ru.yalymar.mapping.model.Model" table="models">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <!-- model -->
        <property name="model" column="model"/>
        <!-- manuf -->
        <many-to-one name="manuf" column="manuf_id" class="ru.yalymar.mapping.model.Manufactor"/>
    </class>
</hibernate-mapping>